<h1> <%= @user.username %>'s Friends </h1>

<div>
  <!-- relationships container -->
  <div>
    <ul>
    	<% @friends.each do |friend| %>
    	<% friend_user = User.find(friend.id) %>
    	<li> <%= link_to friend_user.username, user_path(friend_user) %> </li>
    	<% end %>
    </ul>
  </div>
</div>
