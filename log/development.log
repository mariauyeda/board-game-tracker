 {\"type\"=>\"boardgamecategory\", \"id\"=>\"1002\", \"value\"=>\"Card Game\"}, {\"type\"=>\"boardgamecategory\", \"id\"=>\"1082\", \"value\"=>\"Mythology\"}, {\"type\"=>\"boardgamemechanic\", \"id\"=>\"2080\", \"value\"=>\"Area Control / Area Influence\"}, {\"type\"=>\"boardgamemechanic\", \"id\"=>\"2046\", \"value\"=>\"Area Movement\"}, {\"type\"=>\"boardgamemechanic\", \"id\"=>\"2041\", \"value\"=>\"Card Drafting\"}, {\"type\"=>\"boardgamemechanic\", \"id\"=>\"2664\", \"value\"=>\"Deck / Pool Building\"}, {\"type\"=>\"boardgamemechanic\", \"id\"=>\"2040\", \"value\"=>\"Hand Management\"}, {\"type\"=>\"boardgamemechanic\", \"id\"=>\"2047\", \"value\"=>\"Memory\"}, {\"type\"=>\"boardgamemechanic\", \"id\"=>\"2002\", \"value\"=>\"Tile Placement\"}, {\"type\"=>\"boardgamefamily\", \"id\"=>\"22783\", \"value\"=>\"Admin: Better Description Needed!\"}, {\"type\"=>\"boardgamefamily\", \"id\"=>\"22184\", \"value\"=>\"Admin: Unreleased Games\"}, {\"type\"=>\"boardgamedesigner\", \"id\"=>\"6997\", \"value\"=>\"Christian Martinez\"}, {\"type\"=>\"boardgameartist\", \"id\"=>\"30958\", \"value\"=>\"Dimitri Bielak\"}, {\"type\"=>\"boardgameartist\", \"id\"=>\"44725\", \"value\"=>\"Jim Fitzpatrick\"}, {\"type\"=>\"boardgamepublisher\", \"id\"=>\"5400\", \"value\"=>\"Matagot\"}, {\"type\"=>\"boardgamepublisher\", \"id\"=>\"39\", \"value\"=>\"Pegasus Spiele\"}], \"type\"=>\"boardgame\", \"id\"=>\"155821\"}, \"termsofuse\"=>\"http://boardgamegeek.com/xmlapi/termsofuse\"}}"], ["thumbnail", "http://cf.geekdo-images.com/images/pic3112623_t.jpg"], ["created_at", 2016-09-16 22:49:03 UTC], ["updated_at", 2016-09-16 22:49:03 UTC]]
